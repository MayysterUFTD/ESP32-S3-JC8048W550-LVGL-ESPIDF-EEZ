{
  "version": 4,
  "configurations": [
    {
      "name": "ESP-IDF",
      "compilerPath": "${config:idf.toolsPathWin}\\tools\\xtensa-esp-elf\\esp-15.2.0_20251204\\xtensa-esp-elf\\bin\\xtensa-esp32s3-elf-gcc.exe",
      "compileCommands": "${workspaceFolder}/build/compile_commands.json",
      "includePath": [
        "${workspaceFolder}/**",
        "${config:idf.espIdfPathWin}/components/**",
        "${workspaceFolder}/managed_components/**"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}",
          "${config:idf.espIdfPathWin}/components",
          "${workspaceFolder}/managed_components"
        ],
        "limitSymbolsToIncludedHeaders": false
      },
      "defines": [
        "ESP_PLATFORM",
        "IDF_VER=\"5.4\"",
        "CONFIG_IDF_TARGET_ESP32S3=1"
      ],
      "intelliSenseMode": "gcc-x64",
      "cStandard": "c17",
      "cppStandard": "c++23"
    }
  ]
}
